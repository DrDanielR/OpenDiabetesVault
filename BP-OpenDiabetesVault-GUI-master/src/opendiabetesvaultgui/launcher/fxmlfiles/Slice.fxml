<?import javafx.scene.layout.*?>

<GridPane fx:id="gridPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="677.0" prefWidth="1138.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="opendiabetesvaultgui.slice.SliceController">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" percentWidth="80.0" />
        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" percentWidth="20.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="1.7976931348623157E308" percentHeight="60.0" vgrow="ALWAYS" />
        <RowConstraints percentHeight="40.0" vgrow="ALWAYS" />
    </rowConstraints>
    <children>
        <ListView fx:id="listviewfilterelements" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
            <GridPane.margin>
                <Insets top="6.0" />
            </GridPane.margin>
        </ListView>
        <GridPane GridPane.columnIndex="1" GridPane.rowIndex="1">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Button fx:id="filterbutton" mnemonicParsing="false" onAction="#doFilter" prefHeight="25.0" prefWidth="147.0" text="Filtern" GridPane.rowIndex="1" />
                <Button fx:id="saveFilterCombination" mnemonicParsing="false" onAction="#doSaveFilterCombination" prefHeight="25.0" prefWidth="151.0" text="Speichern" />
                <Button fx:id="loadFilterCombination" mnemonicParsing="false" onAction="#doLoadFilterCombination" prefHeight="25.0" prefWidth="147.0" text="Laden" GridPane.columnIndex="1" />
            </children>
        </GridPane>
        <ScrollPane fx:id="filtercombinationfield" fitToHeight="true" fitToWidth="true" GridPane.rowIndex="1">
            <content>
                <HBox fx:id="filterCombinationHbox" maxWidth="1.7976931348623157E308" />
            </content>
        </ScrollPane>
        <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab text="Dynamic Filter">
                    <content>
                        <LineChart fx:id="filterchart">
                            <xAxis>
                                <CategoryAxis fx:id="filterChartXaxis" side="BOTTOM" />
                            </xAxis>
                            <yAxis>
                                <NumberAxis fx:id="filterChartYaxis" side="LEFT" />
                            </yAxis>
                        </LineChart>
                    </content>
                </Tab>
                <Tab text="Medical Filter">
                    <content>
                        <GridPane>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                <ColumnConstraints />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="333.0" minHeight="10.0" prefHeight="333.0" vgrow="SOMETIMES" />
                                <RowConstraints maxHeight="94.0" minHeight="45.0" prefHeight="45.0" />
                            </rowConstraints>
                            <children>
                                <ImageView fx:id="imageViewForFilter" fitHeight="350.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true" />
                                <HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1">
                                    <children>
                                        <Button fx:id="previousbutton" mnemonicParsing="false" onAction="#getPreviousImage" text="previous" />
                                        <ProgressBar fx:id="importprogressbar" prefHeight="18.0" prefWidth="581.0" progress="0.0" />
                                        <TextField fx:id="currentimport" prefHeight="25.0" prefWidth="52.0" />
                                        <Label fx:id="maximportnumber" text="/ Anzahl" />
                                        <Button fx:id="nextbutton" mnemonicParsing="false" onAction="#getNextImage" text="next" />
                                    </children>
                                </HBox>
                            </children>
                        </GridPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</GridPane>
